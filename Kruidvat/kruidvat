<script type="text/javascript">
var countertablet = 0;
 $("#next-usp").click(function(){  
  if(countertablet == 1){
   $('#nav-2').removeClass('grey-bg');
   countertablet = 0;
   $('#nav-1').addClass('grey-bg');
   $("#usp-1").css("marginLeft","0%");
  } else {
   $('#nav-1').removeClass('grey-bg');
   $('#nav-2').addClass('grey-bg');
    $("#usp-1").css("marginLeft","-100%");
   countertablet++
   }
});

 $("#prev-usp").click(function(){  
  if(countertablet == 1){
   $('#nav-2').removeClass('grey-bg');
   countertablet = 0;
   $('#nav-1').addClass('grey-bg');
   $("#usp-1").css("marginLeft","0%");
  } else {
   $('#nav-1').removeClass('grey-bg');
   $('#nav-2').addClass('grey-bg');
    $("#usp-1").css("marginLeft","-100%");
   countertablet++
   }
});

 $("#nav-1").click(function(){
 $("#nav-1").addClass('grey-bg'); 
 $("#nav-2").removeClass('grey-bg');
 $("#usp-1").css("marginLeft","0%");
 countertablet = 0;
 });
 
 $("#nav-2").click(function(){
 $("#nav-2").addClass('grey-bg'); 
 $("#nav-1").removeClass('grey-bg');
 $("#usp-1").css("marginLeft","-100%");
 countertablet = 1;
 });
 
var counter = 1;
 $("#next-usp-mobile").click(function(){  
  if(counter == 4){
   $('#nav-mob-'+counter).removeClass('grey-bg');
   counter = 1;
   $('#nav-mob-'+counter).addClass('grey-bg');
   const item = document.querySelector('.usp-item');
   const marginLeft = parseInt(item.style.marginLeft.replace('px', '')) || 0;
   item.style.marginLeft = marginLeft + 300 + '%';
  } else {
   $('#nav-mob-'+counter).removeClass('grey-bg');
   $('#nav-mob-'+(counter+1)).addClass('grey-bg');
   const item = document.querySelector('.usp-item');
   const marginLeft = parseInt(item.style.marginLeft.replace('px', '')) || 0;
   item.style.marginLeft = marginLeft - 100 + '%';
   counter++
   }
});

 $("#prev-usp-mobile").click(function(){  
  if(counter == 1){
   $('#nav-mob-'+counter).removeClass('grey-bg');
   counter = 4;
   $('#nav-mob-'+4).addClass('grey-bg');
   const item = document.querySelector('.usp-item');
   const marginLeft = parseInt(item.style.marginLeft.replace('px', '')) || 0;
   item.style.marginLeft = marginLeft - 300 + '%';
  } else {
   $('#nav-mob-'+counter).removeClass('grey-bg');
   $('#nav-mob-'+(counter-1)).addClass('grey-bg');
   const item = document.querySelector('.usp-item');
   const marginLeft = parseInt(item.style.marginLeft.replace('px', '')) || 0;
   item.style.marginLeft = marginLeft + 100 + '%';
   counter--
   }
});
 
  $("#nav-mob-1").click(function(){
 $("#nav-mob-1").addClass('grey-bg'); 
 $("#nav-mob-2").removeClass('grey-bg');
 $("#nav-mob-4").removeClass('grey-bg');
 $("#nav-mob-3").removeClass('grey-bg');
 $("#usp-1").css("marginLeft","0%");
 counter = 1;
 });
 
 $("#nav-mob-1").click(function(){
 $("#nav-mob-1").addClass('grey-bg'); 
 $("#nav-mob-2").removeClass('grey-bg');
 $("#nav-mob-4").removeClass('grey-bg');
 $("#nav-mob-3").removeClass('grey-bg');
 $("#usp-1").css("marginLeft","0%");
 counter = 1;
 });
 
  $("#nav-mob-2").click(function(){
 $("#nav-mob-2").addClass('grey-bg'); 
 $("#nav-mob-1").removeClass('grey-bg');
 $("#nav-mob-3").removeClass('grey-bg');
  $("#nav-mob-4").removeClass('grey-bg');
  $("#usp-1").css("marginLeft","-100%");
  counter = 2;
 });
 
  $("#nav-mob-3").click(function(){
 $("#nav-mob-3").addClass('grey-bg'); 
 $("#nav-mob-2").removeClass('grey-bg');
 $("#nav-mob-4").removeClass('grey-bg');
 $("#nav-mob-1").removeClass('grey-bg');
 $("#usp-1").css("marginLeft","-200%");
 counter = 3;
 });
 
  $("#nav-mob-4").click(function(){
 $("#nav-mob-4").addClass('grey-bg'); 
 $("#nav-mob-1").removeClass('grey-bg');
 $("#nav-mob-3").removeClass('grey-bg');
  $("#nav-mob-2").removeClass('grey-bg');
  $("#usp-1").css("marginLeft","-300%");
  counter = 4;
 });
 
 var mediaQuery = window.matchMedia('(max-width: 992px)');
 
 if (mediaQuery.matches) {
  var persuasionInfo = document.getElementById("persuasion-info");
  document.getElementById("more-info-mobile").addEventListener("click", openDetails);
  function openDetails(e) {
   e.stopPropagation();
   persuasionInfo.classList.toggle("active");
   if (persuasionInfo.classList.contains("active")) {
    document.addEventListener("click", closeDetails);
   }
  }
  function closeDetails() {
   persuasionInfo.classList.remove("active");
   document.removeEventListener("click", closeDetails);
  }
 }
</script>

